{
  "mcpServers": {
    "docker-local": {
      "command": "psn",
      "args": ["mcp", "docker-local"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin"
      }
    },
    "docker-remote": {
      "command": "psn",
      "args": ["mcp", "docker-remote"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin",
        "JUNKPILE_HOST": "junkpile",
        "SSH_KEY": "/Users/chi/.ssh/id_ed25519"
      }
    },
    "ollama": {
      "command": "psn",
      "args": ["mcp", "ollama"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin",
        "JUNKPILE_HOST": "junkpile",
        "SSH_KEY": "/Users/chi/.ssh/id_ed25519"
      }
    },
    "postgres": {
      "command": "psn",
      "args": ["mcp", "postgres"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin",
        "PG_HOST": "localhost",
        "PG_DATABASE": "personality"
      }
    },
    "sqlite": {
      "command": "psn",
      "args": ["mcp", "sqlite"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin"
      }
    },
    "memory": {
      "command": "psn",
      "args": ["mcp", "memory"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin",
        "EMBEDDING_MODEL": "nomic-ai/nomic-embed-text-v1.5",
        "PG_HOST": "localhost",
        "PG_DATABASE": "personality"
      }
    },
    "indexer": {
      "command": "psn",
      "args": ["mcp", "indexer"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin",
        "EMBEDDING_MODEL": "nomic-ai/nomic-embed-text-v1.5",
        "PG_HOST": "localhost",
        "PG_DATABASE": "personality"
      }
    },
    "tts": {
      "command": "psn",
      "args": ["mcp", "tts"],
      "env": {
        "PATH": "/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin"
      }
    }
  }
}
